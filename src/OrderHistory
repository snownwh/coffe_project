CREATE TABLE ORDERHISTORYLIST(
	SEQ_ORDERHISTORY NUMBER(10),
	ID VARCHAR2(20),
	ESPRESSO_BEVERAGES VARCHAR2(50),
	PRICE NUMBER(20),
	ODATE DATE,
    C_SIZE VARCHAR2(20),
    CYRUP VARCHAR2(20), 
    CREAM VARCHAR2(20),
    ADDSHOT VARCHAR2(20),
    AMOUNT NUMBER(20),
    TOTALPRICE NUMBER(20)
);


SELECT * FROM ORDERHISTORYLIST;

CREATE SEQUENCE SEQ_ORDER
START WITH 1
INCREMENT BY 1;

DROP SEQUENCE ORDERHISTORYLIST;

DROP TABLE ORDERHISTORYLIST;